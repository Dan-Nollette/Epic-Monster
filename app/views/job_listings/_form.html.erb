<%= form_for @job_listing, url: profile_job_listings_path(@profile), html: { method: @method } do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %> <br>
  <%= f.label :description %>
  <%= f.text_field :description %> <br>
  <%= f.label :keywords %>
  <%= f.text_field :keywords %> <br>
  <%= f.submit %>
<% end %>
